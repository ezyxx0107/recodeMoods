<block wx:if="{{visiableCalendar}}"><view class="calendar-model"><view class="year-month"><text data-event-opts="{{[['tap',[['computedMonth',['pre']]]]]}}" class="button" bindtap="__e">-</text><text class="year">{{''+year+''}}</text><text>{{''+monthWord[month]+''}}</text><text data-event-opts="{{[['tap',[['computedMonth',['next']]]]]}}" class="button" bindtap="__e">+</text></view><view class="weeks"><block wx:for="{{weeksEnum}}" wx:for-item="item" wx:for-index="__i0__" wx:key="value"><text class="week">{{item.label}}</text></block></view><view class="days"><block wx:for="{{firstDayOfMonthOffset}}" wx:for-item="day" wx:for-index="index"><text class="day"></text></block><block wx:for="{{days}}" wx:for-item="day" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['chooseDateFn',[index+1]]]]]}}" class="{{['day',(index+1===date&&month===$root.g0+1&&year===$root.g1)?'today':'']}}" bindtap="__e">{{index+1}}</text></block></view></view></block>