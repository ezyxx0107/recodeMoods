<view class="moods data-v-5ed3214e"><block wx:for="{{moods}}" wx:for-item="mood" wx:for-index="index" wx:key="index"><view class="row data-v-5ed3214e" style="{{'background-color:'+(emojiColor[mood.emoji])+';'+('color:'+(mood.emoji==='peace'||mood.emoji==='sick'?'#333':'#fff')+';')}}"><text class="day data-v-5ed3214e">{{mood.day<10?'0'+mood.day:mood.day}}</text><image class="emoji data-v-5ed3214e" src="{{emojiImage[mood.emoji]}}" mode></image><text class="mood data-v-5ed3214e">{{mood.mood}}</text><text class="tags data-v-5ed3214e"><block wx:for="{{mood.tags}}" wx:for-item="tag" wx:for-index="ind" wx:key="ind"><text class="tag data-v-5ed3214e">{{''+tag+''}}</text></block></text></view></block><block wx:if="{{moods.length===0}}"><view class="nothing data-v-5ed3214e"><image class="nothing-bg data-v-5ed3214e" src="../static/images/nothing.png" mode></image><text class="nothing-hint data-v-5ed3214e">please record your mood now</text></view></block></view>